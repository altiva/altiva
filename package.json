{
	"name": "@alumna/alumna",
	"version": "3.0.0",
	"description": "Alumna Framework: Development for Humans",
	"repository": {
		"type": "git",
		"url": "git://github.com/alumna/alumna.git"
	},
	"bin": {
		"alumna": "cli.js"
	},
	"scripts": {
		"build": "rollup --config other/rollup/browser.js && rollup --config other/rollup/cli.js",
		"prepare": "npm run build",
		"test": "jest"
	},
	"author": "Paulo Coghi",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/alumna/alumna/issues"
	},
	"jest": {
		"coverageDirectory": "./coverage/",
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/src/utils/**",
			"**/src/generators/*/*.js",
			"**/src/generators/*/*/*.js",
			"**/src/modes/*/*.js",
			"!**/src/modes/all/rsyncAssets.js",
			"!**/src/utils/to.js",
			"!**/src/utils/terser/options.js",
			"!**/src/utils/updateOptions.js",
			"!**/node_modules/**"
		],
		"verbose": true,
		"testEnvironment": "node"
	},
	"devDependencies": {
		"babel-plugin-rewire": "^1.2.0",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"connect-history-api-fallback": "^1.4.0",
		"fs-extra": "^7.0.0",
		"glob": "^7.1.3",
		"hjson": "^3.1.1",
		"jest": "^29.3.1",
		"mri": "^1.1.1",
		"page": "github:alumna/page.js#master",
		"rollup": "^0.67.0",
		"rollup-plugin-commonjs": "^9.2.0",
		"rollup-plugin-json": "^3.1.0",
		"rollup-plugin-node-resolve": "^3.4.0",
		"rollup-plugin-string": "^2.0.2",
		"rollup-plugin-terser": "^3.0.0",
		"rollup-plugin-virtual": "^1.0.1",
		"rsyncwrapper": "^2.0.1",
		"semver-compare": "^1.0.0",
		"svelte": "^2.15.2"
	},
	"dependencies": {
		"browser-sync": "^2.26.3",
		"chokidar": "^2.0.4",
		"degit": "^2.1.3",
		"request": "^2.88.0",
		"terser": "^3.10.11"
	}
}
